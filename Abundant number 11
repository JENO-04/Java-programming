import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
public class Solution {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a =sc.nextInt();
        int b=0,i;
        for(i=1;i<a;i++)
            if(a%i==0){
                b=b+i;
            }
             if(a<b){
                 System.out.println("Abundant Number");
             }
            else{
                System.out.println("Not Abundant Number");
            }
        
    }
}
